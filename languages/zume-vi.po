#: site/course/course-content.php:89 site/course/course-content.php:374
msgid "Spiritual Breathing"
msgstr "Hô hấp thuộc linh"

#: site/course/course-content.php:91 site/course/course-content.php:493
#: site/course/course-content.php:834 site/course/course-content.php:1193
#: site/course/course-content.php:1585 site/course/course-content.php:3011
